# PostgreSQL connection string - REQUIRED
# Replace this with your own PostgreSQL connection string
DATABASE_URL=postgres://username:password@your-postgres-host:5432/database

# Authentication token (optional)
AUTH_TOKEN=

# HTTP host port - the port exposed on your host machine (default: 7432)
HTTP_PORT=7432

# Container internal port - the port inside the container (default: 8080)
CONTAINER_PORT=8080

# Database connection pool
DB_POOL_MIN=5
DB_POOL_MAX=20
DB_POOL_IDLE_TIMEOUT=10000

# Enable response compression
ENABLE_COMPRESSION=true