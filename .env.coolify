# PostgreSQL connection string (set this in Coolify environment variables)
DATABASE_URL=

# Authentication token for proxy security (set this in Coolify environment variables)
AUTH_TOKEN=

# HTTP port (required for Coolify)
PORT=8080

# Database connection pool
DB_POOL_MIN=5
DB_POOL_MAX=20
DB_POOL_IDLE_TIMEOUT=10000

# Enable response compression
ENABLE_COMPRESSION=true